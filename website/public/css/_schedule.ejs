<div id="schedule" class="day-schedule css-schedule container-fluid">

  <div class="row">
    <div class="col-xs-12 col-sm-6">
      <header>
        <h2>Schedule</h2>
        <h3>brought to you by</h3>
      </header>
      <a href="<%- public.sponsors._data.simple.url %>" >
        <img src="<%- public.sponsors._data.simple.logo %>" class="track-sponsor" />
      </a>
    </div>
  </div>


  <table class="table table-hover table-bordered">
    <tbody>
      <tr class="bg-success">
        <td class="time">8:00am</td>
        <td>
