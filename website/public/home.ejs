<%- partial("_partials/home-header") -%>

<main id="teaser">

  <%- partial("_partials/latest-news") -%>

  <%- partial("_partials/venue") -%>

  <div id="preview" class="content">
    <div class="container-fluid">
      <div class="row">

        <div class="col-xs-10 push-xs-1">
          <div class="row">

            <div class="col-xs-12 col-md-4 text-md-center theme css">
              <h3>August 3rd</h3>
              <h2><a href="/css/">CascadiaCSS</a></h2>
            </div>
            <div class="col-xs-12 col-md-4 text-md-center theme browser">
              <h3>August 4th</h3>
              <h2><a href="/browser/">CascadiaJS Browser Day</a></h2>
            </div>
            <div class="col-xs-12 col-md-4 text-md-center theme node">
              <h3>August 5th</h3>
              <h2><a href="/node/">CascadiaJS Node Day</a></h2>
            </div>

          </div>
        </div>

      </div>
    </div>
  </div>


  <%//- partial("_partials/speaker") we can speakers whenever we like. %>

  <div class="small-trees"></div>

  <div id="connect">
    <div class="container">
      <div class="row">
        <%- partial("_partials/connect") %>
      </div>
    </div>
  </div>

  <div class="small-trees reversed"></div>

  <div id="sponsors" class="sp-logos">
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <h3 class="pb1 text-xs-center">Sponsors</h3>
        </div>
      </div>

      <div class="row">
        <% for(var id in public.sponsors._data) { %>
        <div class="col-xs-6 col-sm-4">
          <div class="sp-block">
            <a href="<%- public.sponsors._data[id].url %>" rel="nofollow">
              <img class="" src="<%- public.sponsors._data[id].logo %>" alt="<%- public.sponsors._data[id].name %>" />
            </a>
          </div>
        </div>
        <% } %>
        <hr>
        <div class="col-xs-12 text-sm-center">
          <a href="/sponsors/" class="btn btn-primary">See all of our great sponsors <i class="fa fa-arrow-right"></i></a>
        </div>
      </div>
    </div>
  </div>

  <%- partial("_partials/staff") %>

</main>
