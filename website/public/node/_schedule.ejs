<div id="schedule" class="day-schedule node-schedule container-fluid">

  <div class="row">
    <div class="col-xs-12 col-sm-6">
      <header>
        <h2>Schedule</h2>
        <h3>brought to you by</h3>
      </header>
      <a href="<%- public.sponsors._data.activestate.url %>" class="inline-block">
        <img src="<%- public.sponsors._data.activestate.logo %>" class="track-sponsor" />
      </a>
    </div>
  </div>


  <table class="table table-hover table-bordered">
    <tbody>
      <tr class="bg-success">
        <td class="time">8:00am</td>
        <td>
          <p>
            <span class="uppercase">Breakfast &amp; Registration (8:30am)</span>
          </p>
          <p>
            Registration:
            <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
              Semiahmoo Ballroom
            </button>

            Breakfast:
            <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
              Deck
            </button>
          </p>
        </td>

      </tr>

      <tr class="bg-info">
        <td class="time">9:15am</td>
        <td>
          <span class="uppercase">Opening Ceremony</span>
          <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
            Semiahmoo Ballroom
          </button>
        </td>
      </tr>

      <tr>
        <td class="time">9:30am</td>
        <td>
          <div class="row">
            <div class="col-xs-12 col-sm-7">
              <h4>
                Talks
                <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
                  Semiahmoo Ballroom
                </button>
              </h4>
              <ul class="list-unstyled">
                <%- partial('../_partials/schedule-talk.ejs', {id: 'mariko-kosaka', day: 'node'}) %>
                <%- partial('../_partials/schedule-talk.ejs', {id: 'pawel-szymczykowski', day: 'node'}) %>
                <%- partial('../_partials/schedule-talk.ejs', {id: 'russell-hay', day: 'node'}) %>
              </ul>
            </div>

            <div class="col-xs-12 col-sm-5">
              <div class="workshop">
                <h4>
                  Workshop
                  <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
                    San Juan Ballroom
                  </button>
                </h4>

                <p>
                  Portfolio First - IBM Design Thinking
                </p>

                <a href="<%- public.sponsors._data.ibmdesign.url %>" class="schedule-sponsor-logo">
                  <img src="<%- public.sponsors._data.ibmdesign.logo %>" class="max-height" />
                </a>
              </div>
            </div>
          </div>
        </td>
      </tr>

      <tr class="bg-info">
        <td class="time">11:00am</td>
        <td>
          <span class="uppercase">Coffee Break</span>
        </td>
      </tr>

      <tr>
        <td class="time">11:20am</td>
        <td>
          <div class="row">
            <div class="col-xs-12 col-sm-7">
              <h4>
                Talks
                <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
                  Semiahmoo Ballroom
                </button>
              </h4>
              <ul class="list-unstyled">
                <%- partial('../_partials/schedule-talk.ejs', {id: 'jenn-turner', day: 'node'}) %>
                <%- partial('../_partials/schedule-talk.ejs', {id: 'ivana-mcconnell', day: 'node'}) %>
                <%- partial('../_partials/schedule-talk.ejs', {id: 'bryan-hughes', day: 'node'}) %>
              </ul>
            </div>

            <div class="col-xs-12 col-sm-5">
              <div class="workshop">
                <h4>
                  Workshop
                  <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
                    San Juan Ballroom
                  </button>
                </h4>
                <p>
                  Portfolio First - IBM Design Thinking (continued)
                </p>
              </div>
            </div>
          </div>
        </td>
      </tr>

      <tr class="bg-success">
        <td class="time">12:45pm</td>
        <td>
          <span class="uppercase">Lunch</span>
          <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
            Deck
          </button>
        </td>
      </tr>

      <tr class="">
        <td class="time">2:15pm</td>
        <td>
          <div class="row">
            <div class="col-xs-12 col-sm-7">
              <h4>
                Talks
                <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#semiahmoo-map">
